config wifi-device  radio0
        option type     mac80211
        option channel  11
        option macaddr  00:0c:43:30:52:88
        option hwmode   11ng
        option htmode   HT20
        list ht_capab   GF
        list ht_capab   SHORT-GI-20
        list ht_capab   SHORT-GI-40
        list ht_capab   RX-STBC1

config wifi-iface
        option device   radio0
        option network  lan
        option mode     ap
        option ssid     OpenWRT
        option encryption none
